<!DOCTYPE html>
<html lang="pt-br">
    <%- include('../partials/head'); %>
<body class="container">
    <%- include('../partials/header'); %>
    <main>
        <div class="jumbotron">
            <h1>Node com EJS</h1>
            <p>Essa página será feita com testes pessoais de arquivos EJS</p>
        </div>
        <div class="listaVeiculos">
        <ul>
            <% quatroRodas.forEach(function(carro){ %>
                <li>
                    <strong><%= carro.title %></strong><%= carro.cor %>
                </li>
           <% }) %>
        </ul>
        <ul>
            <% duasRodas.forEach(function(moto){ %>
                <li>
                    <strong><%= moto.title %></strong><%= moto.cor %>
                </li>
           <% }) %>
        </ul>
    </div>
    </main>
    <%- include('../partials/footer'); %>
</body>
</html>